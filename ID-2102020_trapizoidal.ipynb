    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "def f(x):\n",
    "    return x**2 + x - 2\n",
    "\n",
    "a, b = 0, 1\n",
    "n = 1\n",
    "h = (b - a) / n\n",
    "\n",
    "x_vals = np.array([a, b])\n",
    "y_vals = f(x_vals)\n",
    "\n",
    "result = (h / 2) * (y_vals[0] + y_vals[1])\n",
    "print(\"Result =\", result)\n",
    "\n"
   ]
